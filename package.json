{
    "name": "dotfiles",
    "version": "0.0.0",
    "description": "A collection of dotfiles.",
    "scripts": {
        "build:linter-prettier": "npx run-func ./config/helpers/buildLinterConfig.js prettier",
        "build:linter-markdownlint": "npx run-func ./config/helpers/buildLinterConfig.js markdownlint",
        "build:linter-eslint": "npx run-func ./config/helpers/buildLinterConfig.js eslint",
        "sync:dotfiles": "source $PWD/config/helpers/dotfiles && sync_dotfiles",
        "clean:dotfiles": "source $PWD/config/helpers/dotfiles && clean_dotfiles"
    },
    "author": "Nick Snyder",
    "license": "UNLICENSED",
    "devDependencies": {
        "@vitejs/plugin-react": "^4.3.0",
        "autoprefixer": "^10.4.14",
        "browserslist": "^4.21.10",
        "cssnano": "^6.0.1",
        "cssnano-preset-advanced": "^6.0.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.34.3",
        "eslint-plugin-react-hooks": "^4.6.2",
        "fs": "^0.0.1-security",
        "glob": "^10.3.3",
        "lodash": "^4.17.21",
        "lodash.mergewith": "^4.6.2",
        "markdownlint": "^0.29.0",
        "postcss": "^8.4.27",
        "prettier": "^3.0.1",
        "prettier-plugin-css-order": "^2.0.0",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "tailwindcss": "^3.3.3",
        "vite": "^5.2.11",
        "vite-plugin-mkcert": "^1.17.5",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "replace-in-file": "^7.2.0",
        "typescript": "^5.4.5"
    }
}
