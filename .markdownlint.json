{
  "blanks-around-fences": {
    "list_items": true
  },
  "blanks-around-headings": {
    "lines_above": 1,
    "lines_below": 1
  },
  "blanks-around-lists": true,
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "commands-show-output": true,
  "emphasis-style": {
    "style": "underscore"
  },
  "first-line-heading": {
    "level": 1
  },
  "heading-increment": true,
  "heading-start-left": true,
  "heading-style": {
    "style": "atx"
  },
  "hr-style": {
    "style": "consistent"
  },
  "line-length": {
    "code_block_line_length": 120,
    "code_blocks": true,
    "headers": true,
    "heading_line_length": 80,
    "headings": true,
    "line_length": 120,
    "stern": false,
    "strict": false,
    "tables": false
  },
  "link-fragments": true,
  "list-indent": true,
  "ol-prefix": {
    "style": "one"
  },
  "no-alt-text": true,
  "no-bare-urls": true,
  "no-blanks-blockquote": true,
  "no-duplicate-heading": false,
  "no-emphasis-as-heading": {
    "punctuation": ".,;:!?。，；：！？"
  },
  "no-empty-links": true,
  "no-hard-tabs": {
    "code_blocks": true,
    "spaces_per_tab": 2
  },
  "no-inline-html": false,
  "no-missing-space-atx": true,
  "no-multiple-blanks": {
    "maximum": 2
  },
  "no-multiple-space-atx": true,
  "no-multiple-space-blockquote": true,
  "no-reversed-links": true,
  "no-space-in-code": true,
  "no-space-in-emphasis": true,
  "no-space-in-links": true,
  "no-trailing-punctuation": false,
  "no-trailing-spaces": {
    "br_spaces": 2,
    "list_item_empty_lines": false,
    "strict": false
  },
  "reference-links-images": true,
  "single-title": {
    "level": 1
  },
  "single-trailing-newline": true,
  "strong-style": {
    "style": "asterisk"
  },
  "ul-indent": {
    "indent": 2,
    "start_indent": 2,
    "start_indented": true
  },
  "ul-style": {
    "style": "asterisk"
  }
}
